<h1>Otros</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Destinatario</th>
            <td>{{ entity.destinatario }}</td>
        </tr>
        <tr>
            <th>Contenido</th>
            <td>{{ entity.contenido }}</td>
        </tr>
        <tr>
            <th>Remitente</th>
            <td>{{ entity.remitente }}</td>
        </tr>
        <tr>
            <th>Peso</th>
            <td>{{ entity.peso }}</td>
        </tr>
        <tr>
            <th>Fecha</th>
            <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Estado</th>
            <td>{{ entity.estado }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('otros') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('otros_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('otros_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
