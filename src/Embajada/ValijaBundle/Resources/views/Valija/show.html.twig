<h1>Valija</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Fecha</th>
            <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Numerodevalija</th>
            <td>{{ entity.numerodevalija }}</td>
        </tr>
        <tr>
            <th>Observaciones</th>
            <td>{{ entity.observaciones }}</td>
        </tr>
        <tr>
            <th>Cantidaddeitems</th>
            <td>{{ entity.cantidaddeitems }}</td>
        </tr>
        <tr>
            <th>Peso</th>
            <td>{{ entity.peso }}</td>
        </tr>
        <tr>
            <th>Peso</th>
            <td>{{ entity.pdf }}</td>
        </tr>        
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('valija') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('valija_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('valija_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
