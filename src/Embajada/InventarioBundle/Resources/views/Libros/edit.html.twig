{% extends "::base8.html.twig" %}
{% block body %}
<div class="row-row-fluid">
    <div class="span12">
        <div class="box">
            <div class="box-title">
                <h3>
                    <i class="icon-list"></i>Editar
                </h3>
            </div>
        </div>
        <div class="box-content">
        <form action="{{ path('Libros_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
            {{ form_widget(edit_form) }}
            <p>
                <button type="submit">Edit</button>
            </p>
        </form>

<ul class="record_actions">
    <li>
        <a href="{{ path('Libros') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock body %}
