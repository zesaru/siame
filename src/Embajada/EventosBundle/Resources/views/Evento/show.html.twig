{% extends "::base8.html.twig" %}
{% block body %}

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Vocativo</th>
            <td>{{ entity.vocativo }}</td>
        </tr>
        <tr>
            <th>Cantidad</th>
            <td>{{ entity.cantidad }}</td>
        </tr>
        <tr>
            <th>Horadeinvitacion</th>
            <td>{{ entity.horadeinvitacion }}</td>
        </tr>
        <tr>
            <th>Fechalainvitacion</th>
            <td>{{ entity.fechalainvitacion|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('evento') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('evento_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('evento_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock body %}