<h1>Notascd</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Numerodenota</th>
            <td>{{ entity.numerodenota }}</td>
        </tr>
        <tr>
            <th>Cuerpo</th>
            <td>{{ entity.cuerpo }}</td>
        </tr>
        <tr>
            <th>Sumilla</th>
            <td>{{ entity.sumilla }}</td>
        </tr>
        <tr>
            <th>Fecha</th>
            <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Idioma</th>
            <td>{{ entity.idioma }}</td>
        </tr>
        <tr>
            <th>Destinatario</th>
            <td>{{ entity.destinatario }}</td>
        </tr>
        <tr>
            <th>Aprobado</th>
            <td>{{ entity.aprobado }}</td>
        </tr>
        <tr>
            <th>Observaciones</th>
            <td>{{ entity.observaciones }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('notascd') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('notascd_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('notascd_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
